package pers.ethan.file;

import java.io.File;

/**
 * Hello world!
 *
 */
public class App 
{
    public static void main( String[] args )
    {
        System.out.println( "Hello World!" );
    }
    
    public static void showFile(String fileName){
    	File file = new File(fileName);
    	if (!file.exists()) {
			System.out.println("file does not exists");
		}
    }
}
