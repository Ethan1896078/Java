package pers.ethan.homework;

public abstract class Employee {
	/** 姓名 */
	private String name;
	/** 年龄 */
	private int age;
	/** 月薪 */
	private double salary; 
	/** 入职时间 */
	private String joindedDate;
	
	/**
	 * desc:工作
	 * <p>创建人：huangzhe , 2016年5月19日下午5:32:00</p>
	 */
	public abstract void work();
}
