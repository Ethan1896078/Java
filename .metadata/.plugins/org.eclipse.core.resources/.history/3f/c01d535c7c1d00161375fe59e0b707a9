package pers.ethan.homework;

public class DeepCopy {
	public static void main(String[] args) {
		C c = new C(3);
	}
}

class A{
	private int aInt;
	private B b;
	
	public A(int aInt, B b) {
		this.aInt = aInt;
		this.b = b;
	}
}

class B{
	private int bInt;
	private C c;
	
	public B(int bInt, C c) {
		this.bInt = bInt;
		this.c = c;
	}
}

class C{
	private int cInt;
	
	public C(int cInt){
		this.cInt = cInt;
	}
}

