package pers.ethan.homework;

public abstract class AbstractSensor implements Sensable{
	/** 正常范围的最低温度 */
	private double min;
	/** 正常范围的最高温度 */
	private double max;
	
	public void sense(double value) {
		if (value < min || value > max) {
			warn();
			adjust();
		}
	}
	
	protected abstract void warn();
	
	protected abstract void adjust();
}
